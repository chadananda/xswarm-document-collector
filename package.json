{
  "name": "xswarm-document-collector",
  "version": "1.0.0",
  "description": "Terse, robust document collection framework with extensible connectors for Gmail, Drive, websites, Dropbox, Facebook, and academic repositories",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "xswarm-collect": "./src/cli/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "document-collection",
    "gmail",
    "google-drive",
    "dropbox",
    "web-scraping",
    "arxiv",
    "facebook",
    "oauth2",
    "data-extraction",
    "xswarm"
  ],
  "author": "XSwarm Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xswarm/xswarm-document-collector.git"
  },
  "bugs": {
    "url": "https://github.com/xswarm/xswarm-document-collector/issues"
  },
  "homepage": "https://github.com/xswarm/xswarm-document-collector#readme",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests",
    "format": "prettier --write 'src/**/*.js' 'tests/**/*.js'"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^12.0.0",
    "googleapis": "^130.0.0",
    "mammoth": "^1.7.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.0",
    "pdf-parse": "^1.1.1",
    "pino": "^8.19.0",
    "simple-oauth2": "^5.0.0",
    "sql.js": "^1.10.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.2.0",
    "dotenv": "^16.4.0",
    "eslint": "^8.56.0",
    "pino-pretty": "^13.1.3",
    "vitest": "^1.2.0"
  }
}
